{
  "expo": {
    "name": "plantmanager",
    "slug": "plantmanager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.markusdel.plantmanager"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Projeto feito na NLW#5, que consistir e um projeto em react native para gerenciar plantinhas",
    "githubUrl": "https://github.com/MarKus-del/NLW5-PlantManager"
  }
}
